<plugin>  
    <extension point="org.wso2.developerstudio.kernel.wso2plugin.template">
      <template
           id="org.wso2.artifact.plugin.sample"
           name="Single Module Project"
           archive="samples/SampleArtifactPlugin.zip"
           icon="icons/single-artifact.png"
           description="%Sampleartifactplugin_description" >       
      </template>
      <template
           id="org.wso2.multiple.artifacts.plugin.sample"
           name="Multi Module Project"
           archive="samples/SampleMultipleArtifactPlugin.zip"
           icon="icons/multiple-artifacts.png"
           description="%Multipleartifactplugin_description" >       
      </template>
      <template
           id="org.wso2.perspective.plugin.sample"
           name="Plug-in with a Perspective"
           archive="samples/PerspectivePluginSample.zip"
           icon="icons/perspective.png"
           description="%Perspectiveplugin_description" >       
      </template>
       <template
           id="org.wso2.view.plugin.sample"
           name="Plugin with a View"
           archive="samples/ViewPluginSample.zip"
           icon="icons/view.png"
           description="%Viewplugin_description" >       
      </template>
   </extension>
</plugin>
